package com.lcomputerstudy.books.java.ch08.test;

public class Ex02test {

	public static void main(String[] args) {
		
		Circle circle = new Circle();
		circle.p=new Point(10,10);
		System.out.println(circle.p.x);
		System.out.println(circle.p.y);
	}

}


class Circle{
	Point p;
	int r;
}


class Point{
	int x;
	int y;
	
	Point(int x, int y){
		this.x=x;
		this.y=y;
	}
}


